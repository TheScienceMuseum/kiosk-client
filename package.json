{
  "name": "kiosk-v4",
  "productName": "kiosk-v4",
  "version": "1.0.0",
  "private": true,
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "yarn start-ui & yarn start-app",
    "start-ui": "cd src/ui && BROWSER=none SKIP_PREFLIGHT_CHECK=true ./node_modules/.bin/react-scripts start",
    "start-app": "APPLICATION_ENV=prod electron-forge start",
    "package": "electron-forge package",
    "test": "cd src/ui && react-scripts test",
    "make": "cd src/ui && react-scripts build && cd ../../ && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color",
    "postinstall": "cd src/ui && yarn install"
  },
  "keywords": [],
  "author": "lawrence",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "kiosk_v4"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "kioskv4"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.3",
    "electron-config": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "3.0.2",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7"
  }
}
